<?xml version="1.0" encoding="UTF-8"?>
<div xmlns:c="http://java.sun.com/jsp/jstl/core" 
	xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields" xmlns:form="urn:jsptagdir:/WEB-INF/tags/form" 
	xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" 
	 xmlns:util="urn:jsptagdir:/WEB-INF/tags/util"
	 version="2.0">
    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
    <jsp:output omit-xml-declaration="yes"/>
    <form:update id="fu_de_stefanocke_japkit_roo_petclinic_domain_owner" modelAttribute="updateOwnerCommand" path="/owners/updateOwner" versionField="Version" z="0">
  	<field:select field="pets" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_pets" itemValue="id" items="${petsChoices}" path="petss" required="false" z="0"/>
     	
    	<field:input field="firstName" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_firstname" required="false"  z="0"/>
    
    	<field:input field="lastName" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_lastname" required="true"  z="0"/>
    
     	<util:panel id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_address" title="address" openPane="true">
     	
    	<field:input field="address.street" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_address_street" required="true"  z="0"/>
    
    	<field:input field="address.city" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_address_city" required="true"  z="0"/>
    
    	<field:input field="address.zip" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_address_zip" required="true"  z="0"/>
    
    	<field:input field="address.state" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_address_state" required="false"  z="0"/>
    
     	</util:panel><br/>	
     	
    	<field:input field="telephone" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_telephone" required="true"  z="0"/>
    
    	<field:input field="homePage" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_homepage" required="false"  z="0"/>
    
    	<field:input field="email" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_email" required="false"  z="0"/>
    <field:datetime field="birthDay" id="c_de_stefanocke_japkit_roo_petclinic_domain_owner_birthday" dateTimePattern="${owner_birthday_date_format}" past="false" required="true" z="0"/>
        </form:update>
    
</div>